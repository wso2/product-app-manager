<head>
    <title>{{include title}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="{{url "/themes/default/css/bootstrap.min.css"}}" rel="stylesheet" media="screen">
    <link href="{{url "/themes/default/css/bootstrap-responsive.min.css"}}" rel="stylesheet">
    <!-- fontawesome -->
    <link rel="stylesheet" href="{{url "/themes/default/css/font-awesome.min.css"}}">


    <!-- Token input -->
    <link rel="stylesheet" href="{{url "/themes/default/js/token.input/css/token-input-facebook.css"}}">

    <!-- publisher styles -->
    <link href="{{url "/themes/default/css/es-publisher-commons.css"}}" rel="stylesheet" media="screen">
	<link href="{{url "/themes/default/css/navigation.css"}}" rel="stylesheet" media="screen">
    <script src="{{url "/themes/default/js/modernizr.custom.js"}}"></script>
    {{css}}
</head>
<body>
    {{ include header}}
<div class="container-assets container">
	<div class="ribbon-cont">
	    <div class="row-fluid ribbon">
	        {{include ribbon}}
	    </div>
    </div>
    <div class="row-fluid content-area">
        <div class="span3 publisher-left">
            {{include leftnav}}
        </div>
        <div class="span9 publisher-right">
            {{include listassets}}
        </div>
    </div>
    <div class="row-fluid publisher-footer">
        <div class="span12">
            {{>footer}}
        </div>
    </div>
</div>

<script src="{{url "/themes/default/js/jquery-1.10.2.min.js"}}"></script>
<script src="{{url "/themes/default/js/bootstrap.min.js"}}"></script>
<script src="{{url "/themes/default/js/jquery.dlmenu.js"}}"></script>
